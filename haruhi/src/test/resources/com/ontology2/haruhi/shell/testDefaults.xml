<?xml version="1.0" encoding="UTF-8"?>
<!-- this file defines default values for properties that normally are set in
     the user's home directory;  we need these so tests can properly test
     initialization of the applicationContext.xml and bogus values are OK
     because we can't touch AWS in a unit test -->
     
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans.xsd
         http://www.springframework.org/schema/context
         http://www.springframework.org/schema/context/spring-context.xsd">
   <bean name="awsCredentials" class="com.amazonaws.auth.BasicAWSCredentials">
        <constructor-arg value="mick"/>
        <constructor-arg value="jagger"/>
   </bean>
   <bean name="awsSoftwareBucket" class="java.lang.String">
      <constructor-arg value="s3://bakemono/" />
   </bean>
</beans>
     